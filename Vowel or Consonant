include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

  char ch;
    scanf("%c", &ch);
    if ((ch >= 'A' && ch <= 'Z') || (ch >= 'a' && ch <= 'z')) {
        char lower = (ch >= 'A' && ch <= 'Z') ? ch + 32 : ch;

        if (lower == 'a' || lower == 'e' || lower == 'i' || lower == 'o' || lower == 'u')
            printf("Vowel\n");
        else
            printf("Consonant\n");
    } else {
        printf("Invalid Input\n");
    }  
    return 0;
}
